{
  "extends": "../../../../.eslintrc.json",
  "overrides": [
    {
      "files": ["*.ts"],
      "rules": {
        "@angular-eslint/component-selector": [
          "error",
          {
            "prefix": ["", "daming"],
            "style": "kebab-case",
            "type": "element"
          }
        ],
        "@angular-eslint/directive-selector": [
          "error",
          {
            "prefix": ["", "daming"],
            "style": "camelCase",
            "type": "attribute"
          }
        ]
      }
    }
  ]
}
