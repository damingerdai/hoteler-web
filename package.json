{
  "name": "hoteler-web",
  "version": "0.0.0",
  "engines": {
    "node": "^14.15.0 || >=16.10.0",
    "yarn": ">=1.22.4 <2",
    "npm": "Please use yarn instead of NPM to install dependencies"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "changelog:all": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "test": "ng test",
    "test:ci": "ng test --browsers=ChromeHeadless --watch=false --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "version:patch": "yarn version --patch",
    "version:minor": "yarn version --minor",
    "prepare": "husky install",
    "analyze": "ng build --configuration production --stats-json && webpack-bundle-analyzer dist/hoteler/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.3.11",
    "@angular/cdk": "13.3.8",
    "@angular/common": "13.3.11",
    "@angular/compiler": "13.3.11",
    "@angular/core": "13.3.11",
    "@angular/flex-layout": "13.0.0-beta.38",
    "@angular/forms": "13.3.11",
    "@angular/material": "13.3.8",
    "@angular/platform-browser": "13.3.11",
    "@angular/platform-browser-dynamic": "13.3.11",
    "@angular/router": "13.3.11",
    "apexcharts": "3.35.3",
    "ngx-apexcharts": "0.0.1",
    "rxjs": "7.5.5",
    "tslib": "2.4.0",
    "zone.js": "0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.3.7",
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "13.3.7",
    "@angular/compiler-cli": "13.3.11",
    "@angular/language-service": "13.3.11",
    "@commitlint/cli": "17.0.2",
    "@commitlint/config-conventional": "17.0.2",
    "@types/jasmine": "4.0.3",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "17.0.38",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "conventional-changelog-cli": "2.2.2",
    "eslint": "7.32.0",
    "husky": "8.0.1",
    "jasmine-core": "4.1.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.3.20",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.0.0",
    "karma-jasmine-html-reporter": "1.7.0",
    "lint-staged": "12.4.3",
    "prettier": "2.6.2",
    "protractor": "7.0.0",
    "ts-node": "10.7.0",
    "typescript": "4.5.5",
    "webpack-bundle-analyzer": "4.5.0"
  },
  "resolutions": {}
}
