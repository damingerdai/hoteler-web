{
  "name": "hoteler-web",
  "version": "0.0.0",
  "engines": {
    "node": "^14.20.0 || ^16.13.0 || >=18.10.0",
    "yarn": ">=1.22.4 <2",
    "npm": "Please use yarn instead of NPM to install dependencies"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:online": "yarn ng serve --proxy-config=src/proxy.conf.online.json --hmr",
    "build": "ng build --configuration production",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "changelog:all": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "test": "ng test",
    "test:ci": "ng test --browsers=ChromeHeadless --watch=false --code-coverage",
    "lint": "yarn lint:ts && yarn lint:scss",
    "lint:ts": "ng lint",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint:scss:fix": "stylelint \"src/**/*.scss\" --fix",
    "e2e": "ng e2e",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "version:patch": "yarn version --patch",
    "version:minor": "yarn version --minor",
    "prepare": "is-ci || husky install",
    "analyze": "ng build --configuration production --stats-json && webpack-bundle-analyzer dist/hoteler/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "15.1.4",
    "@angular/cdk": "15.1.4",
    "@angular/common": "15.1.4",
    "@angular/compiler": "15.1.4",
    "@angular/core": "15.1.4",
    "@angular/flex-layout": "15.0.0-beta.42",
    "@angular/forms": "15.1.4",
    "@angular/material": "15.1.4",
    "@angular/platform-browser": "15.1.4",
    "@angular/platform-browser-dynamic": "15.1.4",
    "@angular/router": "15.1.4",
    "apexcharts": "3.37.0",
    "ngx-apexcharts": "0.1.1",
    "ngx-dom-confetti": "^0.1.0",
    "perf-marks": "^1.14.2",
    "rxjs": "7.8.0",
    "tslib": "2.5.0",
    "zone.js": "0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.1.5",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "15.1.5",
    "@angular/compiler-cli": "15.1.4",
    "@angular/language-service": "15.1.4",
    "@commitlint/cli": "17.4.2",
    "@commitlint/config-conventional": "17.4.2",
    "@types/jasmine": "4.3.1",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "18.14.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "conventional-changelog-cli": "2.2.2",
    "eslint": "^8.34.0",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "jasmine-core": "4.5.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.1",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "lint-staged": "13.1.0",
    "prettier": "2.8.3",
    "protractor": "7.0.0",
    "stylelint": "^14.16.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-order": "^6.0.2",
    "stylelint-scss": "^4.3.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.5",
    "webpack-bundle-analyzer": "4.8.0"
  },
  "resolutions": {}
}
