{
  "name": "hoteler-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "changelog:all": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "test": "ng test",
    "test:ci": "ng test --browsers=ChromeHeadless --watch=false --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "sh scripts/postinstall/ngcc.sh && patch-package",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "version:patch": "yarn version --patch",
    "version:minor": "yarn version --minor",
    "prepare": "husky install",
    "analyze": "ng build --configuration production --stats-json && webpack-bundle-analyzer dist/hoteler/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.0.0",
    "@angular/cdk": "~13.0.0",
    "@angular/common": "~13.0.0",
    "@angular/compiler": "~13.0.0",
    "@angular/core": "~13.0.0",
    "@angular/flex-layout": "^12.0.0-beta.34",
    "@angular/forms": "~13.0.0",
    "@angular/material": "^13.0.0",
    "@angular/platform-browser": "~13.0.0",
    "@angular/platform-browser-dynamic": "~13.0.0",
    "@angular/router": "~13.0.0",
    "apexcharts": "^3.29.0",
    "ng-apexcharts": "^1.5.12",
    "rxjs": "~6.6.7",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.0",
    "@angular-eslint/builder": "12.5.0",
    "@angular-eslint/eslint-plugin": "12.6.1",
    "@angular-eslint/eslint-plugin-template": "12.6.1",
    "@angular-eslint/schematics": "12.6.1",
    "@angular-eslint/template-parser": "12.6.1",
    "@angular/cli": "~13.0.0",
    "@angular/compiler-cli": "~13.0.0",
    "@angular/language-service": "~13.0.0",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@types/jasmine": "~3.8.1",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^16.7.10",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.30.0",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^7.32.0",
    "husky": "^7.0.4",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.7",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^11.1.2",
    "patch-package": "^6.4.7",
    "prettier": "^2.4.1",
    "protractor": "~7.0.0",
    "ts-node": "~10.2.1",
    "typescript": "~4.4.4",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "resolutions": {}
}
