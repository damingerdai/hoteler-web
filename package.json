{
  "name": "hoteler-web",
  "version": "0.0.0",
  "engines": {
    "node": "^16.14.0 || >=18.10.0",
    "yarn": ">=1.22.4 <2",
    "npm": "Please use yarn instead of NPM to install dependencies"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:online": "yarn ng serve --proxy-config=src/proxy.conf.online.json --hmr",
    "build": "ng build --configuration production",
    "build:esbuild": "ng run hoteler-web:esbuild",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "changelog:all": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "test": "ng test",
    "test:ci": "ng test --browsers=ChromeHeadless --watch=false --code-coverage",
    "lint": "yarn lint:ts && yarn lint:scss",
    "lint:ts": "ng lint",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint:scss:fix": "stylelint \"src/**/*.scss\" --fix",
    "e2e": "ng e2e",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "version:patch": "yarn version --patch",
    "version:minor": "yarn version --minor",
    "prepare": "is-ci || husky install",
    "analyze": "ng build --configuration production --stats-json && webpack-bundle-analyzer dist/hoteler/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "17.0.2",
    "@angular/cdk": "17.0.0",
    "@angular/common": "17.0.2",
    "@angular/compiler": "17.0.2",
    "@angular/core": "17.0.2",
    "@angular/forms": "17.0.2",
    "@angular/material": "17.0.0",
    "@angular/platform-browser": "17.0.2",
    "@angular/platform-browser-dynamic": "17.0.2",
    "@angular/router": "17.0.2",
    "@angular/service-worker": "17.0.2",
    "apexcharts": "3.44.0",
    "ngx-apexcharts": "0.4.2",
    "ngx-dom-confetti": "^0.2.0",
    "ngx-is-standalone": "0.3.0",
    "perf-marks": "^1.14.2",
    "rxjs": "7.8.1",
    "tfontfaceobserver": "^0.0.2",
    "tslib": "2.6.2",
    "zone.js": "0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.0.1",
    "@angular-eslint/builder": "17.0.1",
    "@angular-eslint/eslint-plugin": "17.0.1",
    "@angular-eslint/eslint-plugin-template": "17.0.1",
    "@angular-eslint/schematics": "17.1.0",
    "@angular-eslint/template-parser": "17.0.1",
    "@angular/cli": "17.0.1",
    "@angular/compiler-cli": "17.0.2",
    "@angular/language-service": "17.0.2",
    "@commitlint/cli": "18.4.1",
    "@commitlint/config-conventional": "17.8.1",
    "@types/jasmine": "4.3.5",
    "@types/jasminewd2": "2.0.11",
    "@types/node": "20.9.2",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "conventional-changelog-cli": "4.1.0",
    "eslint": "^8.53.0",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "jasmine-core": "5.1.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.2",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "lint-staged": "14.0.1",
    "postcss": "^8.4.23",
    "postcss-scss": "^4.0.6",
    "prettier": "3.1.0",
    "protractor": "7.0.0",
    "stylelint": "^15.2.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended-scss": "^12.0.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "^5.0.0",
    "ts-node": "10.9.1",
    "typescript": "5.2.2",
    "webpack-bundle-analyzer": "4.10.1"
  },
  "resolutions": {}
}
