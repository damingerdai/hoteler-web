{
  "name": "hoteler-web",
  "version": "0.0.0",
  "engines": {
    "node": "^16.14.0 || >=18.10.0",
    "yarn": ">=1.22.4 <2",
    "npm": "Please use yarn instead of NPM to install dependencies"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:online": "yarn ng serve --proxy-config=src/proxy.conf.online.json --hmr",
    "build": "ng build --configuration production",
    "build:esbuild": "ng run hoteler-web:esbuild",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "changelog:all": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "test": "ng test",
    "test:ci": "ng test --browsers=ChromeHeadless --watch=false --code-coverage",
    "lint": "yarn lint:ts && yarn lint:scss",
    "lint:ts": "ng lint",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint:scss:fix": "stylelint \"src/**/*.scss\" --fix",
    "e2e": "ng e2e",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "version:patch": "yarn version --patch",
    "version:minor": "yarn version --minor",
    "prepare": "is-ci || husky install",
    "analyze": "ng build --configuration production --stats-json && webpack-bundle-analyzer dist/hoteler/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "16.0.2",
    "@angular/cdk": "16.0.0",
    "@angular/common": "16.0.2",
    "@angular/compiler": "16.0.2",
    "@angular/core": "16.0.2",
    "@angular/forms": "16.0.2",
    "@angular/material": "16.0.0",
    "@angular/platform-browser": "16.0.2",
    "@angular/platform-browser-dynamic": "16.0.2",
    "@angular/router": "16.0.2",
    "apexcharts": "3.40.0",
    "ngx-apexcharts": "0.1.2",
    "ngx-dom-confetti": "^0.2.0",
    "perf-marks": "^1.14.2",
    "rxjs": "7.8.1",
    "tfontfaceobserver": "^0.0.2",
    "tslib": "2.5.2",
    "zone.js": "0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.0.2",
    "@angular-eslint/builder": "16.0.1",
    "@angular-eslint/eslint-plugin": "16.0.1",
    "@angular-eslint/eslint-plugin-template": "16.0.1",
    "@angular-eslint/schematics": "16.0.1",
    "@angular-eslint/template-parser": "16.0.1",
    "@angular/cli": "16.0.2",
    "@angular/compiler-cli": "16.0.2",
    "@angular/language-service": "16.0.2",
    "@commitlint/cli": "17.6.3",
    "@commitlint/config-conventional": "17.6.3",
    "@types/jasmine": "4.3.1",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "20.2.1",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.2",
    "conventional-changelog-cli": "2.2.2",
    "eslint": "^8.39.0",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "jasmine-core": "5.0.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.2",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "lint-staged": "13.2.2",
    "postcss": "^8.4.23",
    "postcss-scss": "^4.0.6",
    "prettier": "2.8.8",
    "protractor": "7.0.0",
    "stylelint": "^15.2.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended-scss": "^11.0.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "^5.0.0",
    "ts-node": "10.9.1",
    "typescript": "5.0.4",
    "webpack-bundle-analyzer": "4.8.0"
  },
  "resolutions": {}
}
