{
    "name": "hoteler-web",
    "version": "0.0.0",
    "engines": {
        "node": "^18.10.0 || >=20.10.0",
        "yarn": ">=1.22.4 <2",
        "npm": "Please use yarn instead of NPM to install dependencies"
    },
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "start:online": "yarn ng serve --proxy-config=src/proxy.conf.online.json --hmr",
        "build": "ng build --configuration production",
        "build:esbuild": "ng run hoteler-web:esbuild",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
        "changelog:all": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
        "test": "ng test",
        "test:ci": "ng test --browsers=ChromeHeadless --watch=false --code-coverage",
        "lint": "yarn lint:ts && yarn lint:scss",
        "lint:ts": "ng lint",
        "lint:scss": "stylelint \"src/**/*.scss\"",
        "lint:scss:fix": "stylelint \"src/**/*.scss\" --fix",
        "e2e": "ng e2e",
        "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
        "version:patch": "yarn version --patch",
        "version:minor": "yarn version --minor",
        "prepare": "is-ci || husky",
        "analyze": "ng build --configuration production --stats-json"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "17.3.5",
        "@angular/cdk": "17.3.3",
        "@angular/common": "17.3.5",
        "@angular/compiler": "17.3.5",
        "@angular/core": "17.3.5",
        "@angular/forms": "17.3.5",
        "@angular/material": "17.3.3",
        "@angular/material-experimental": "^17.3.3",
        "@angular/platform-browser": "17.3.5",
        "@angular/platform-browser-dynamic": "17.3.5",
        "@angular/router": "17.3.5",
        "@angular/service-worker": "17.3.5",
        "apexcharts": "3.48.0",
        "ngx-apexcharts": "0.4.12",
        "ngx-dom-confetti": "0.3.0",
        "ngx-is-standalone": "0.3.0",
        "perf-marks": "^1.14.2",
        "rxjs": "7.8.1",
        "tfontfaceobserver": "^0.0.2",
        "tslib": "2.6.2",
        "zone.js": "0.14.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "17.3.3",
        "@angular-eslint/builder": "17.3.0",
        "@angular-eslint/eslint-plugin": "17.3.0",
        "@angular-eslint/eslint-plugin-template": "17.3.0",
        "@angular-eslint/schematics": "17.3.0",
        "@angular-eslint/template-parser": "17.3.0",
        "@angular/cli": "17.3.3",
        "@angular/compiler-cli": "17.3.5",
        "@angular/language-service": "17.3.5",
        "@commitlint/cli": "18.4.3",
        "@commitlint/config-conventional": "18.4.3",
        "@types/jasmine": "4.6.4",
        "@types/jasminewd2": "2.0.13",
        "@types/node": "20.10.5",
        "@typescript-eslint/eslint-plugin": "^7.2.0",
        "@typescript-eslint/parser": "^7.2.0",
        "conventional-changelog-cli": "4.1.0",
        "eslint": "^8.57.0",
        "husky": "9.0.11",
        "is-ci": "3.0.1",
        "jasmine-core": "5.1.1",
        "jasmine-spec-reporter": "7.0.0",
        "karma": "6.4.2",
        "karma-chrome-launcher": "3.2.0",
        "karma-coverage": "2.2.1",
        "karma-jasmine": "5.1.0",
        "karma-jasmine-html-reporter": "2.1.0",
        "lint-staged": "15.2.0",
        "postcss": "^8.4.23",
        "postcss-scss": "^4.0.6",
        "prettier": "3.2.5",
        "protractor": "7.0.0",
        "stylelint": "^16.1.0",
        "stylelint-config-recommended-scss": "^14.0.0",
        "stylelint-config-standard": "^36.0.0",
        "stylelint-config-standard-scss": "^12.0.0",
        "stylelint-scss": "^6.0.0",
        "ts-node": "10.9.2",
        "typescript": "5.2.2"
    }
}
